{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended", "standard"],
  "env": {
    "jest": true,
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/prop-types": 0,
    "comma-dangle": 0,
  },
}
